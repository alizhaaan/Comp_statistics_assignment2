M').astype(int)\n","\n","original_coefs, p_values, permuted_coefs, original_coefs = crt_logistic_regression(X, y)\n","\n","print(\"Original Coefficients:\", original_coefs)\n","print(\"P-values:\", p_values)\n","\n","permuted_coefs_array = np.array(permuted_coefs)\n","\n","coef_index = 30  # Choose any index in range from 0 to 60\n","plt.hist(permuted_coefs_array[:, coef_index], bins=30, alpha=0.7, label='Permuted Coefficients')\n","plt.axvline(x=original_coefs[coef_index], color='red', linestyle='--', label='Original Coefficient')\n","plt.title('Distribution of Permuted Coefficients')\n","plt.xlabel('Coefficient Value')\n","plt.ylabel('Frequency')\n","plt.legend()\n","plt.show()\n"],"metadata":{"id":"H-o0wP0qod7e","colab":{"base_uri":"https://localhost:8080/","height":802},"executionInfo":{"status":"ok","timestamp":1711118237920,"user_tz":-60,"elapsed":180074,"user":{"displayName":"Алижан Жапан","userId":"12195284568563934566"}},"outputId":"9cd5f431-6c59-47ec-d07f-958b479e339c"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Original Coefficients: [ 0.28039893  0.33833483  0.29871002  0.65757032  0.5113003   0.08640533\n"," -0.10059622  0.00831914  0.92832961  1.05906481  1.61970803  1.43383225\n","  0.7658062  -0.00726896 -0.36386833 -0.5994015  -0.50210637 -0.26558587\n","  0.35971113  0.61702387  0.67766039  0.49135321  0.41416994  0.39053017\n"," -0.26732154 -0.3064264   0.06065647  0.47523751  0.32641874  0.33419937\n"," -0.94292167  0.13969045  0.03468528 -0.42431454 -0.50961723 -1.15835567\n"," -0.84324713  0.38798843  0.53442551 -0.56933387  0.14147475  0.67625995\n","  0.94942408  1.08604967  1.48536625  1.17160483  0.80144519  0.83510554\n","  0.53317826  0.02627981  0.14595838  0.13814682  0.05250237  0.07349646\n","  0.0092967   0.02676172 -0.00798177  0.06520138  0.06950908  0.03461346]\n","P-values: [2.540e-02 4.630e-02 1.376e-01 5.200e-03 7.450e-02 7.676e-01 7.320e-01\n"," 9.791e-01 6.000e-03 2.900e-03 0.000e+00 0.